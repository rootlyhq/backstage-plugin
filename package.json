{
  "name": "@rootly/backstage-plugin",
  "version": "1.2.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
